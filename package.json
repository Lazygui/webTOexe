{
  "name": "web-vue",
  "version": "1.0.0",
  "description": "A desktop application to launch Vue3+TS+Vite projects",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.vue3.project.launcher",
    "productName": "web-vue",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "extraFiles": [
      {
        "from": ".",
        "to": "resources/app",
        "filter": [
          "app/**/*"
        ]
      }
    ]
  },
  "keywords": [
    "electron",
    "vue3",
    "vite",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "find-process": "^1.4.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "electron-builder": "^24.12.0",
    "electron": "^29.1.0"
  }
}